# Yamix Environment Configuration
# Generated by Ansible - do not edit directly

# Application
WEB_URL=https://{{ yamix_domain }}

# Server binding (for nginx reverse proxy)
YAMIX_HOST=127.0.0.1
YAMIX_PORT={{ yamix_port | default(3026) }}

# Database
POSTGRES_PASSWORD={{ yamix_postgres_password }}

# OpenAI API (required for AI counseling)
OPENAI_API_KEY={{ yamix_openai_api_key }}

# JWT Secret (for authentication)
JWT_SECRET={{ yamix_jwt_secret }}
